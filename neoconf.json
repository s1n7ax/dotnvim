{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": true
    }
  },
  "neoconf": {
    "plugins": {
      "jsonls": {
        "enabled": true
      },
      "lua_ls": {
        "enabled": true
      },
      "jdtls": {
        "enabled": true
      },
      "cssls": {
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "Lua.format.enable": false,
      "Lua.semantic.enable": false,
      "Lua.workspace": {
        "maxPreload": 10000,
        "preloadFileSize": 10000
      },
      "Lua.telemetry": false
    },
    "jsonls": {
      "json.format.enable": false
    }
  }
}
